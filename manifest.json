{
    "name":"Google Meet Attendance Check",
    "description":"Google Attendance Check by GITA Autonomous College",  
    "version":"0.4",
    "manifest_version":2,
    "content_scripts":[
      {
        "matches":[
          "https://meet.google.com/*"
        ],
        "js":["content.js"]
      }
    ],
      "browser_action": {
        "default_icon": "./images/logo32.png",
        "default_popup": "popup.html"
      },
      "icons": { 
        "16": "./images/logo16.png",
        "48": "./images/logo48.png",
       "128": "./images/logo128.png" 
      }
}
